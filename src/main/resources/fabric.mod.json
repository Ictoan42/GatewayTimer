{
  "schemaVersion": 1,
  "id": "gatewaytimer",
  "version": "${version}",
  "name": "GatewayTimer",
  "description": "Adds a 1 second timer to the villager trading GUI when in The End, to make void trading easier",
  "authors": ["Ictoan42"],
  "contact": {
    "repo": "https://github.com/Ictoan42/GatewayTimer"
  },
  "license": "GPL-3.0",
  "icon": "assets/gatewaytimer/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "ictoan.gatewaytimer.client.GatewayTimerClient"
    ],
    "main": [
      "ictoan.gatewaytimer.GatewayTimer"
    ]
  },
  "mixins": [
    "gatewaytimer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
